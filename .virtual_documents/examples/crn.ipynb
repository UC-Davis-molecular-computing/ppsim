def get_discrete_averaging(m):
    # create reactions with m species s0, ..., s(m-1)
    states = pp.species(' '.join(['s'+str(i) for i in range(m)]))
    rxns = []
    for i in range(m):
        for j in range(i):
            avg = (i + j) / 2
            rxns.append(states[i] + states[j] >> states[math.floor(avg)] + states[math.ceil(avg)])
    return states, rxns


